<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bridge.dao.BridgeDao">
    <resultMap id="bridgeMapper" type="com.bridge.entity.Bridge">
    </resultMap>
    <select id="findById" parameterType="Integer" resultMap="bridgeMapper">
        select * from tb_bridge where id = #{param}
    </select>
    <select id="findAll" resultMap="bridgeMapper">
        select * from tb_bridge;
    </select>
</mapper>